meta {
  name: FinishGame
  type: http
  seq: 5
}

post {
  url: {{baseUrl}}/api/finishGame/{{gameId}}?x-player-secret={{adminSecret}}
  body: none
  auth: none
}

params:query {
  x-player-secret: {{adminSecret}}
}

script:post-response {
  if (res.status === 200) {
    bru.setEnvVar("gameStatus", res.body.status);
  }
}
